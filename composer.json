{
  "name": "sno/app",
  "description": "Layered Web Application for managing cleaning staff and payroll (ShineO).",
  "type": "project",
  "authors": [
    {
      "name": "Nirajan Ghimirey",
      "email": "nirajanghimirey@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-pdo": "*",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
